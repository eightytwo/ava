%tr{class: cycle("odd", "even")}
  %td.thin
    = member.user.organisation_display_name

  - if current_user.can_manage?(folio)
    %td.thin.admin_column
      = link_to t("links.edit"), edit_folio_user_path(member, fid: folio)
      = render "shared/link_separator"
      = link_to t("links.delete"), member, confirm: t("folio_user.delete.confirm"), method: :delete
